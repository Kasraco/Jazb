@model List<Jazb.DomainClasses.Entities.AzmooneducatedSkill>
@using Jazb.Web
<table class="table table-bordered table-striped  ">
    <thead>
        <tr>
            <th>کد</th>
            <th>عنوان</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var t in Model)
            {
            <tr>
                <td>@t.AzmooneducatedSkillCode.ToString()</td>
                <td>@t.AzmooneducatedSkillTitle</td>
                <td>
                 


                    @Ajax.ActionLink(" ", MVC.Admin.AzmoonSetting.ActionNames.DeleteAzmooneducatedSkills, MVC.Admin.AzmoonSetting.Name, new { Id = t.Id }, new AjaxOptions { HttpMethod = "DELETE", InsertionMode = InsertionMode.Replace, UpdateTargetId = "AzmoonEducationListList", OnSuccess = "loadAjaxComponents", OnComplete = "hideLoading", OnBegin = "showLoading", Confirm = "آیا برای حذف اطمینان دارید؟" }, new { @class = " fa fa-minus-square fa-2x ", style = "color:red;" })






                </td>
            </tr>
        }
    </tbody>
</table>

@Html.Raw(TempData["Message"])


