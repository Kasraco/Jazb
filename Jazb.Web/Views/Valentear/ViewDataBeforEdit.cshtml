                                        var imagePic_Conscript = string.Format("data:image/gif;base64,{0}", imageBase64Pic_Conscript);

                                        <img class="fileupload-preview thumbnail" src="@imagePic_Conscript" style="width: 240px; height: 100%; min-height: 128px;" />
                                    }
                                }
                              
