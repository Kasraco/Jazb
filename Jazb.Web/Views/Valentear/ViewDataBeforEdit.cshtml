@model Jazb.Model.ValentearModel.EditRegisterModel
@using Jazb.Web.Helpers


@{
    ViewBag.Title = "ویرایش ثبت نام";
    Layout = "~/Views/Shared/_LayoutAzmoon.cshtml";

    var HtmlClassPicture = "";
    var DontShowClass = "";
    var IsBehvarz = "";
}

<style type="text/css">
    .styled_checkbox {
        cursor: pointer;
        border-radius: 5px;
        margin: 3px;
        padding: 0px 5px;
    }

    .styled_list {
        padding: 3px 10px 3px 5px;
        line-height: 25px;
    }

    .col-md-2,
    .control-label {
        text-align: right;
    }
</style>

    @{
        if (ViewBag.IsManualPayment == "Yes")
        {
            HtmlClassPicture = "col col-md-4";
            DontShowClass = "none";
        }
        else
        {
            HtmlClassPicture = "col col-md-3";
            DontShowClass = "";
        }
    }   


