                <legend>اطلاعات محل سکونت</legend>
                <h4>نشانی کامل محل سکونت</h4>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            @Html.Label("استان", new { @class = "control-label col-md-2" })
                            <div class="col-md-3">
                                <div class="form-control">@Model.Valentear.Address_ProvinceName</div>

                            </div>
                            @Html.Label("شهرستان", new { @class = "control-label col-md-1" })
                            <div class="col-md-3">
                                <div class="form-control">@Model.Valentear.Address_CityName</div>

                            </div>

                            @Html.Label("بخش", new { @class = "control-label col-md-1" })
                            <div class="col-md-2">
                                <div class="form-control">@Model.Valentear.Address_ZoneName</div>

                            </div>
                        </div>

                        <div class="form-group">
                            @Html.Label("کد پستی", new { @class = "control-label col-md-2" })
                            <div class="col-md-3">
                                <div class="form-control">@Model.Valentear.PostalCode</div>

                            </div>
                            @Html.Label("تلفن ثابت", new { @class = "control-label col-md-1" })
                            <div class="col-md-2">
                                <div class="form-control">@Model.Valentear.Tel</div>

                            </div>

                            @Html.Label("تلفن همراه", new { @class = "control-label col-md-1" })
                            <div class="col-md-3">
                                <div class="form-control">@Model.Valentear.Mobile</div>

                            </div>
                        </div>
                        <div class="form-group">
                            @Html.Label("شماره تلفن برای تماس ضروری", new { @class = "control-label col-md-3" })
                            <div class="col-md-9">
                                <div class="form-control">@Model.Valentear.TellNeed</div>

                            </div>
                        </div>

                    </div>
                </div>
            </fieldset>
        </div>
        <div class="well" style="word-wrap:break-word; text-wrap:normal;">
            <fieldset>
                <legend></legend>
                @{
                    var qSignVal = string.Format(ViewBag.PossibleSignTextValenter, Model.Valentear.FirstName, Model.Valentear.LastName);

                }
                @Html.Raw(qSignVal)

                <div class="center">
                    @Html.ActionLink("اطلاعات بالا صحیح نیست - بازگشت", "EditRegister", "Valentears", new { AID=Model.Valentear.AzmoonId}, new { @class = "btn btn-warning" })
                    <input type="submit" value="موافقم" title="موافقم" class="btn btn-success " />
                </div>
            </fieldset>
        </div>

        
                    }

</div>

