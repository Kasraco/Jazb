@model Jazb.DomainClasses.Entities.FAQ
@using Jazb.Web.Helpers

@{
    ViewBag.Title = "Edit";
}

@section ContentPanel{



<div class="container" id="divpanle">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12">
			

    @using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

        @Html.HiddenFor(model => model.Id)
        @Html.HiddenFor(model => model.Body)
        @Html.HiddenFor(model => model.FirstName)
        @Html.HiddenFor(model => model.LastName)
        @Html.HiddenFor(model => model.MeliCode)
        @Html.HiddenFor(model => model.QuestionDate)
        @Html.HiddenFor(model => model.Subject)

		@Html.Partial("_CreateOrEdit", Model)

         <div class="row">
            <div class="col-md-4">
                  <input type="submit" value="Save" class="btn btn-primary btn-block" />
            </div>
         <div class="col-md-2">
		      @Html.ActionLink("Back To List", "Index", null, new { @class = "btn btn-warning" })
		 </div>
      </div>
}
    </div>
   </div>
        
</div>

}
